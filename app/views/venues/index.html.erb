<header class="w3-container w3-center w3-padding-32"> 
  <h2 class="w3-wide"><i class="fa fa-music w3-margin-right"></i>Venues</h2>
</header>
<div class="w3-container w3-content w3-center w3-padding-64">
<div class="w3-row-padding">
  <div class="w3-card white">
    <div class="w3-container w3-black">
      <h3>Explore Venues</h3>
    </div>
    <% @venues.each do |venue| %>
    <ul class="w3-ul w3-border-top">
      <li>
        <h3><%= link_to venue.name, venue, style:"text-decoration:none" %></h3>
        <p><%= venue.address %></p>
      </li>
    <% end %>
    </ul>
    <div class="w3-container w3-black w3-large"><span class="w3-right"><%= link_to 'Add Venue', new_venue_path, style:"text-decoration:none"%></span></div>
  </div>
</div>
</div>