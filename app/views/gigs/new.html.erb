<h1>Add Gig</h1>

<%= form_for [@venue, @gig] do |f| %>
  Name of Event: <%= f.text_field :name %><br />
  Date: <%= f.date_field :date %><br />
  Time <%= f.time_field :time %><br />
  Admission: $<%= f.text_field :cost %> <br>
  Bands: <%= f.text_area :band %> <br>
  Details: <%= f.text_area :description %> <br>
  Related Links: <%= f.url_field :link %> <br>

  <%= f.submit %>
<% end %>
<button>
  <%= link_to 'Cancel', venue_gigs_path(@venue.id) %>
</button>